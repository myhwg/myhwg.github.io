  <LayerCatalog title="天地图">
      <LayerDefine id="6">
      <caption>OpenStreetMap</caption>
      <name>OpenStreetMap</name>
      <description>OpenStreetMap</description>
      <legend></legend>
      <url_templet>http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png</url_templet>
      <url_templet_subdomains>a,b,c</url_templet_subdomains>
      <min_level>0</min_level>
      <max_level>18</max_level>
      <projectType>EPSG_3857</projectType>
      <mapbounds>
        <left>-2.0037508342789244E7</left>
        <top>2.0037508342789244E7</top>
        <right>2.0037508342789244E7</right>
        <bottom>-2.0037508342789244E7</bottom>
      </mapbounds>
    </LayerDefine>
  	 <LayerDefine id="7">
	    <caption>标准地图</caption>
	    <name>tianditu_vec</name>
	    <url_templet>http://{s}.tianditu.com/DataServer?T=vec_c&amp;x={x}&amp;y={y}&amp;l={z}</url_templet>
	    <url_templet_subdomains>t0,t1,t2,t3,t4,t5,t6,t7</url_templet_subdomains>
	    <min_level>1</min_level>
	    <max_level>18</max_level>
	    <projectType>EPSG_4326</projectType>
	    <mapbounds>
	      <left>-180.0</left>
	      <top>90.0</top>
	      <right>180.0</right>
	      <bottom>-90.0</bottom>
	    </mapbounds>
	  </LayerDefine>
    <LayerDefine id="8">
	    <caption>影像地图</caption>
	    <name>tianditu_img</name>
	    <url_templet>http://{s}.tianditu.com/DataServer?T=img_c&amp;x={x}&amp;y={y}&amp;l={z}</url_templet>
	    <url_templet_subdomains>t0,t1,t2,t3,t4,t5,t6,t7</url_templet_subdomains>
	    <min_level>1</min_level>
	    <max_level>18</max_level>
	    <projectType>EPSG_4326</projectType>
	    <mapbounds>
	      <left>-180.0</left>
