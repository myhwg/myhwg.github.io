  <LayerCatalog title="天地图">
  	  <LayerDefine id="7">
	    <caption>标准地图</caption>
	    <name>标准地图</name>
	    <url_templet>http://{s}.tianditu.com/DataServer?T=vec_c&amp;x={x}&amp;y={y}&amp;l={z}</url_templet>
	    <url_templet_subdomains>t1,t2,t3</url_templet_subdomains>
	    <min_level>1</min_level>
	    <max_level>18</max_level>
	    <projectType>EPSG_4326</projectType>
	    <mapbounds>
	      <left>-180.0</left>
	      <top>90.0</top>
	      <right>180.0</right>
	      <bottom>-90.0</bottom>
	    </mapbounds>
	  </LayerDefine>
  </LayerCatalog>
