<?xml version="1.0" encoding="utf-8"?>
<LayerDefines>
  <LayerCatalog title="Google地图">
  	<LayerDefine id="1">
	    <caption>道路地图</caption>
	    <name>道路地图</name>
	    <description>Google　道路地图</description>
	    <legend>http://www.google.cn/maps</legend>
	    <url_templet>http://{s}.google.cn/vt/lyrs=m&hl=zh-CN&gl=cn&x={x}&y={y}&z={z}</url_templet>
	    <url_templet_subdomains>mt0,mt1,mt2,mt3</url_templet_subdomains>
	    <min_level>0</min_level>
	    <max_level>18</max_level>
	    <projectType>EPSG_3857</projectType>
	    <mapbounds>
	      <left>-2.0037508342789244E7</left>
	      <top>2.0037508342789244E7</top>
	      <right>2.0037508342789244E7</right>
	      <bottom>-2.0037508342789244E7</bottom>
	    </mapbounds>
